@{
    ViewBag.Title = "General Forms";
    ViewBag.SubTitle = "Company";
}

        <div class="page-title">
            <div class="row">
                <div class="col-12 col-md-6 order-md-1 order-last">
                    <h3>@ViewBag.Title</h3>
                    <p class="text-subtitle text-muted">General Setup Forms</p>
                </div>
                <div class="col-12 col-md-6 order-md-2 order-first">
                    <nav aria-label="breadcrumb" class="breadcrumb-header float-start float-lg-end">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item"><a href="index.html">Dashboard</a></li>
                            <li class="breadcrumb-item active" aria-current="page">@ViewBag.SubTitle</li>
                        </ol>
                    </nav>
                </div>
            </div>
        </div>

        <section id="multiple-column-form">
            <div class="row match-height">
                <div class="col-12">
                    <div class="card">
                        <div class="card-header">
                            <h4 class="card-title">@ViewBag.SubTitle</h4>
                        </div>
                        <div class="card-content">
                            <div class="card-body">
                                <form class="form">
                                    <div class="row">
                                        <div class="col-md-6 col-12">
                                            <div class="form-group">
                                                <label for="Company_Name">Name</label>
                                                <input type="text" id="Company_Name" class="form-control" placeholder="Company Name" name="Company_Name-column">
                                            </div>
                                        </div>
                                        <div class="col-md-6 col-12">
                                            <div class="form-group">
                                                <label for="company_desc-column">Descritption</label>
                                                <input type="text" id="company_desc-column" class="form-control" placeholder="Company Description" name="company_desc-column">
                                            </div>
                                        </div>
                                        <div class="col-md-6 col-12">
                                            <div class="form-group">
                                                <label for="city-column">Address</label>
                                                <input type="text" id="Company_address-column" class="form-control" placeholder="Address" name="Company_address-column">
                                            </div>
                                        </div>
                                        <div class="col-md-6 col-12">
                                            <div class="form-group">
                                                <label for="Company_phones-column">Phone</label>
                                                <input type="text" id="Company_phones-column" class="form-control" name="Company_phones-column" placeholder="Phones">
                                            </div>
                                        </div>
                                        <div class="col-md-6 col-12">
                                            <div class="form-group">
                                                <label for="Company_fax-column">FAX</label>
                                                <input type="text" id="Company_fax-column" class="form-control" name="Company_fax-column" placeholder="FAX">
                                            </div>
                                        </div>
                                        <div class="col-md-6 col-12">
                                            <div class="form-group">
                                                <label for="Company_email-column">Email</label>
                                                <input type="email" id="Company_email-column" class="form-control" name="Company_email-column" placeholder="Email">
                                            </div>
                                        </div>
                                        <div class="col-md-6 col-12">
                                            <div class="form-group">
                                                <label for="CityName-column">City</label>
                                                <input type="text" id="CityName-column" class="form-control" name="CityName-column" placeholder="City">
                                            </div>
                                        </div>
                                        <div class="col-md-6 col-12">
                                            <div class="form-group">
                                                <label for="PostCode-column">Postal Code</label>
                                                <input type="text" id="PostCode-column" class="form-control" name="PostCode-column" placeholder="Post Code">
                                            </div>
                                        </div>
                                        <div class="col-md-6 col-12">
                                            <div class="form-group">
                                                <label for="STaxRegNo-column">Saletax Reg. No.</label>
                                                <input type="number" id="STaxRegNo-column" class="form-control" name="STaxRegNo-column" placeholder="Saletax Registered No.">
                                            </div>
                                        </div>
                                        <div class="col-md-6 col-12">
                                            <div class="form-group">
                                                <label for="NationalTaxNo-column">National Tax No.</label>
                                                <input type="number" id="NationalTaxNo-column" class="form-control" name="NationalTaxNo-column" placeholder="National Tax No.">
                                            </div>
                                        </div>

                                        <div class="col-12 d-flex justify-content-end">
                                            <button type="submit" class="btn btn-primary me-1 mb-1">Submit</button>
                                            <button type="reset" class="btn btn-light-secondary me-1 mb-1">Reset</button>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
       